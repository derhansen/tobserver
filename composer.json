{
  "name": "derhansen/tobserver",
  "type": "typo3-cms-extension",
  "description": "tObserver TYPO3 extension required to send instance data to tobserver.com",
  "homepage": "https://github.com/derhansen/tobserver",
  "support": {
    "issues": "https://github.com/derhansen/tobserver/issues"
  },
  "authors": [
    {
      "name": "Torben Hansen",
      "email": "torben@derhansen.com",
      "homepage": "http://www.derhansen.com",
      "role": "Developer"
    }
  ],
  "keywords": [
    "TYPO3 CMS",
    "typo3 monitoring",
    "tobserver.com"
  ],
  "license": [
    "GPL-2.0-or-later"
  ],
  "replace": {
    "typo3-ter/tobserver": "self.version"
  },
  "autoload": {
    "psr-4": {
      "Derhansen\\Tobserver\\": "Classes"
    }
  },
  "require": {
    "typo3/cms-core": "^9.5 || ^10.4 || ~11.0",
    "ext-json": "*"
  }
}
